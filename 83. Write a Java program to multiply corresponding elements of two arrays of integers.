import java.util.Arrays;

public class codeforces {
    public static void main(String[] args){
        int[] array1={1, 3, -5, 4};
        int[] array2={1, 4, -5, -2};
        int[] new_array= {(array1[0]*array2[0]),(array1[1]*array2[1]),(array1[2]*array2[2]),(array1[3]*array2[3])};
        System.out.println("Result:"+Arrays.toString(new_array));




    }
}
