import java.util.Scanner;
 
public class Main {
 
 public static void main(String[] args) {
   
  System.out.print("Input a number: ");
  
  Scanner in = new Scanner(System.in);
  int num = in.nextInt();
  
  for (int i=0; i<10; i++){
   System.out.println(num + " x " + (i+1) + " = " + 
   (num * (i+1)));
  }
 }
}
