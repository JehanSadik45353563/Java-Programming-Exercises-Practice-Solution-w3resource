import java.util.Arrays;

public class codeforces {
    public static void main(String[] args){
        int [] array1 = {5, 7};
        System.out.println("Original Array:"+Arrays.toString(array1));
        System.out.println(
                        array1[0]==4 ||
                        array1[0]==7 ||
                        array1[0]==4 ||
                        array1[1]==7);
    }
}
